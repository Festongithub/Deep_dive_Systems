#include <stdio.h>

/**
 * main - runs the code
 * @price : in dollars
 * @quantity: in kgs
 **/
int main()
{
  int price;
  int quantity;
  
  printf("Enter the price:");
  scanf("%d", &price);
  printf("Enter the quantity: ");
  scanf("%d", &quantity);
  int revenue = price * quantity;
  printf("Total revenue is: %d\n", revenue);
  return (0);
}
